<%= form_for @sales_rep do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name%><br>

  <%= f.label :company %><br>
  <%= f.text_field :company%><br>

  <%= f.label :category %><br>
  <%= f.text_field :category%><br>

  <%= f.label :phone %><br>
  <%= f.text_field :phone%><br>

  <%= f.label :email %><br>
  <%= f.text_field :email%><br>

  <%= f.hidden_field :user_id, value: current_user.id %>

  <%= f.submit %>

<% end %>
